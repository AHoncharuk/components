extends /blocks/layout/layout

include ../blocks/group/group

block head
  - var pageTitle = 'Components';

block content
  +b(class='slack')&attributes(attributes)
    +e('aside')(class='sidebar')
      +group(getData('starred'))
      +group(getData('channels'))
      +group({ title: 'Direct messages', count: 5, label: 'Create new channel', items: [{ name: 'netology', icon: { name: 'add' } }, { name: 'netology', icon: { name: 'add' } }, { name: 'netology', icon: { name: 'add' } }, { name: 'netology', icon: { name: 'add' } }] })
    +e('header')(class='header')
      | dssfssdfsdf
    +e('main')(class='main')
    +e('footer')(class='footer')
